{
  "manifest_version": 2,
  "name": "Share Detector",
  "version": "0.1.0",
  "description": "Chrome extension to detect share links by selecting 通过划选探测分享链接",
  "homepage_url": "https://github.com/myl7/share-detector",
  "icons": {
    "16": "icon_60x60.png",
    "48": "icon_60x60.png",
    "128": "icon_200x200.png"
  },
  "background": {
    "scripts": ["bundle.js"]
  },
  "permissions": ["contextMenus", "tabs", "notifications"]
}
